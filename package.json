    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "*",
    "babel-jest": "^22.4.3",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^3.0.1",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "jest": "^22.4.3",
